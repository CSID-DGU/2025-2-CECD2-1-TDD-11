# AWS Configuration
AWS_REGION=ap-northeast-2
ECR_REGISTRY=<your-aws-account-id>.dkr.ecr.ap-northeast-2.amazonaws.com

# S3 Configuration
S3_DEPLOY_BUCKET=bucket-name-for-deployment

# Key Pairs (각 서비스별 다른 키)
KEY_PAIR_SERVER=server-key
KEY_PAIR_AI=ai-key
KEY_PAIR_STREAM=stream-key

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=talktobook
DB_USER=postgres
DB_PASSWORD=your-db-password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6380

# ALB Configuration
ALB_ARN=arn:aws:elasticloadbalancing:ap-northeast-2:your-account-id:loadbalancer/app/your-alb-name/your-alb-id
# 또는 ALB 태그로 찾기
ALB_TAG_NAME=your-alb-name

# Listener ARN (선택사항)
LISTENER_ARN=arn:aws:elasticloadbalancing:ap-northeast-2:your-account-id:listener/app/your-alb-name/your-listener-id/your-listener-rule-id
